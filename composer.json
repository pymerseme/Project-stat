{
  "name": "pymerseme/project-stat",
  "description": "ITEA PHP Advanced 6th homework.",
  "keywords": ["project", "template", "structure", "skeleton", "phpunit"],
  "type": "project",
  "license": "",
  "authors": [
    {
      "name": "Alexander Ordin",
      "email": "pymerseme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
